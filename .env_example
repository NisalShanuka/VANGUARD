# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=replace_with_a_random_long_secret

# Discord Provider
# Set redirect URI to: http://localhost:3000/api/auth/callback/discord
DISCORD_CLIENT_ID=replace_with_discord_oauth_client_id
DISCORD_CLIENT_SECRET=replace_with_discord_oauth_client_secret
# Required for auto-join: bot token from your Discord app bot user
DISCORD_BOT_TOKEN=replace_with_discord_bot_token
# Required for auto-join: the server (guild) ID users should be added to
DISCORD_GUILD_ID=replace_with_discord_guild_id

# Database Configuration (UCP / Application DB)
DB_HOST=mysql.gravelhost.com
DB_USER=replace_with_db_user
DB_PASSWORD=replace_with_db_password
DB_NAME=replace_with_db_name

# Game Database (FiveM / QBCore / ESX)
GAME_DB_HOST=mysql.gravelhost.com
GAME_DB_USER=replace_with_game_db_user
GAME_DB_PASSWORD=replace_with_game_db_password
GAME_DB_NAME=replace_with_game_db_name

# FiveM Live Admin API
FIVEM_API_URL=replace_with_fivem_api_url
FIVEM_API_TOKEN=replace_with_fivem_api_token
